                            android:textAlignment="viewStart"
                            android:textSize="32sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:id="@+id/ThridLL1_6"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="27dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Description:" />

                            <TextView
                                android:id="@+id/recentDate6"
                                android:layout_width="100dp"
                                android:layout_height="32dp"
                                android:layout_marginEnd="16dp"
                                android:layout_weight="1"
                                android:text="TextView"
                                android:textAlignment="textStart"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ThirdLL2_6"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="# of Tracked Stats:" />

                            <TextView
                                android:id="@+id/statsNumber6"
                                android:layout_width="100dp"
                                android:layout_height="32dp"
                                android:layout_marginEnd="16dp"
                                android:layout_weight="1"
                                android:text="TextView"
                                android:textAlignment="textEnd"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ThirdLL3_6"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="# of Total Entries:" />

                            <TextView
                                android:id="@+id/entryNumber6"
                                android:layout_width="100dp"
                                android:layout_height="32dp"
                                android:layout_marginEnd="16dp"
                                android:layout_weight="1"
                                android:text="TextView"
                                android:textAlignment="textEnd"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/ThirdLL4_6"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/timer6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
